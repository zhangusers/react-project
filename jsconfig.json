{
	"compilerOptions": {
        "jsx": "react",
        "baseUrl": "./",
		"paths": {
			"@/*": ["src/*"],
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/components/*"],
			"@pages/*": ["src/pages/*"],
			"@store/*": ["src/store/*"]
        },
        "checkJs": true,
		"experimentalDecorators": true
    },
    "include": ["src/**/*"],
	"exclude": ["node_modules", "build"]
}